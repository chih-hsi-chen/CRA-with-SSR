{
  "name": "ssr-cra",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@material-ui/core": "^4.9.11",
    "@material-ui/icons": "^4.9.1",
    "@testing-library/jest-dom": "^4.2.4",
    "@testing-library/react": "^9.5.0",
    "@testing-library/user-event": "^7.2.1",
    "compression": "^1.7.4",
    "express": "^4.17.1",
    "isomorphic-fetch": "^2.2.1",
    "npm-run-all": "^4.1.5",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-helmet": "^6.0.0",
    "react-redux": "^7.2.0",
    "react-router-config": "^5.1.1",
    "react-router-dom": "^5.1.2",
    "react-scripts": "3.4.1",
    "redux": "^4.0.5",
    "redux-thunk": "^2.3.0",
    "serialize-javascript": "^3.0.0",
    "styled-jsx": "^3.2.5",
    "webpack-merge": "^4.2.2",
    "webpack-node-externals": "^1.7.2"
  },
  "scripts": {
    "start": "node server_build/bundle.js",
    "build": "rm -rf server_build && npm run client:build && npm run build-server",
    "test": "react-app-rewired test",
    "eject": "react-scripts eject",
    "client:build": "react-app-rewired build",
    "client:start": "react-app-rewired start",
    "dev": "npm run build-server && npm-run-all --parallel dev:*",
    "build-server": "webpack --config webpack.server.js",
    "dev:server-watch": "webpack --config webpack.server.js --watch",
    "dev:client-watch": "cra-build-watch",
    "dev:server": "nodemon --watch server_build --exec \"node server_build/bundle.js\""
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "babel-plugin-transform-class-properties": "^6.24.1",
    "cra-build-watch": "^3.2.0",
    "customize-cra": "^0.9.1",
    "ignore-loader": "^0.1.2",
    "nodemon": "^2.0.3",
    "react-app-rewired": "^2.1.5",
    "webpack-cli": "^3.3.11"
  }
}
